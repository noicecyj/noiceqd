{
  "plugins": [
    [
      "build-plugin-fusion",
      {
        "themePackage": "@alifd/theme-design-pro"
      }
    ],
    [
      "build-plugin-moment-locales",
      {
        "locales": [
          "zh-cn"
        ]
      }
    ]
  ],
  "proxy": {
    "/pageMenuApi": {
      "enable": true,
      "target": "http://127.0.0.1:8050/cyj-page-menu"
    },
    "/entityCreateApi": {
      "enable": true,
      "target": "http://127.0.0.1:8050/cyj-entity-creater"
    },
    "/dictionaryApi": {
      "enable": true,
      "target": "http://127.0.0.1:8050/cyj-dictionary"
    },
    "/logApi": {
      "enable": true,
      "target": "http://127.0.0.1:8050/cyj-log"
    },
    "/ssoApi": {
      "enable": true,
      "target": "http://127.0.0.1:8050/cyj-sso"
    },
    "/sqlApi": {
      "enable": true,
      "target": "http://127.0.0.1:8050/cyj-query"
    }
  }
}